{
  "name": "rest-web",
  "version": "2.6.0",
  "description": "REST Server Web",
  "main": "app.js",
  "scripts": {
    "dev": "tsnd --respawn --clear --inspect -- src/app.ts",
    "prisma:migrate:prod": "prisma migrate deploy",
    "build": "rimraf ./dist && tsc && npm run prisma:migrate:prod",
    "docker:test": "docker compose -f docker-compose.test.yaml --env-file .env.test up -d",
    "prisma-migrate-test": "dotenv -e .env.test -- prisma migrate deploy",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --silent",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node dist/app.js"
  },
  "keywords": [
    "typescript",
    "node",
    "express"
  ],
  "author": "Daniel Gonzalez",
  "license": "UNLICENSED",
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@types/jest": "^29.5.12",
    "dotenv": "^16.3.2",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "prisma": "^5.8.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@types/yargs": "^17.0.32",
    "dotenv-cli": "^7.3.0",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "yargs": "^17.7.2"
  }
}
